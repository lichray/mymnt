complete mymnt.sh	'c/-/(r D a l h L p o)/' \
	'n@-L@`ls /usr/share/locale`@' 'n/-p/d/' 'n/-o/x:<options>/' \
	'n/*/`mymnt.sh -l`/'
complete umymnt.sh	'c/-/(a D l h p)/' 'n/-p/d/' 'n/*/`umymnt.sh -l`/'

